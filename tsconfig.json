{
  "ts-node": {
    "compilerOptions": {
      "target": "es5",
      "lib": ["esnext"],
      "allowJs": true,
      "skipLibCheck": true,
      "strict": false,
      "forceConsistentCasingInFileNames": true,
      "noEmit": true,
      "esModuleInterop": true,
      "module": "CommonJS",
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "isolatedModules": false,
      "jsx": "preserve",
      "incremental": true,
      "baseUrl": ".",
      "paths": {
        "*": ["*", "IBS/*"]
      }
    }
  },
  "include": ["custom.d.ts", "**/*.ts", "**/*.tsx"],
  "exclude": ["node_modules"]
}